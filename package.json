{
  "name": "mlg-frontend",
  "version": "0.0.0",
  "description": "The frontend for an AI-app",
  "keywords": [
    "machine learning",
    "artificial intelligence",
    "ml",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PAJALN1K/mlgFrontend"
  },
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"react-scripts start\" \"gulp\"",
    "build": "react-scripts build",
    "prettier": "prettier --write src/**/*.{ts,tsx,json}",
    "lint:ts": "npx eslint src/**/*.{ts,tsx}",
    "lint:ts:fix": "npx eslint src/**/*.{ts,tsx} --fix",
    "lint:css": "stylelint src",
    "lint:css:fix": "stylelint src --fix",
    "check-types": "npx tsc --noEmit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.8",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "mlg": "file:",
    "mlg-frontend": "file:",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.1",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-boundaries": "^4.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gulp": "^5.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.1.0",
    "prettier": "3.3.3",
    "sass": "^1.80.5",
    "stylelint": "^16.9.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">= 20.16.0",
    "npm": ">= 10.8.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
